<!DOCTYPE html>
<html>
    <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-COMPATIBLE" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1 maximum-scale=1.0, user-scalable=no">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{% block title %}Club Tijuana Xoloitzcuintles de Caliente{% endblock %}</title>
    {% block stylesheets %}
    <link rel="icon" type="image/x-icon" href="{{ asset('l-t.png') }}">
    <link rel="stylesheet" href="/bundles/icfrontend/css/bootstrap.min.css">
    <link rel="stylesheet" href="/bundles/icfrontend/css/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/bundles/icfrontend/css/app.css">
    {% endblock %}

    </head>
    <body>

    {% include'IcFrontendBundle::Default/partials/menu.html.twig' %}


    <div class="container-fluid" id="container">

        <div class="row">

            {% for flashMessage in app.session.flashbag.get('success') %}
                <div class="alert alert-success" role="alert">
                    {{ flashMessage }}
                </div>
            {% endfor %}
            {% for flashMessage in app.session.flashbag.get('warning') %}
                <div class="alert alert-warning" role="alert">
                    {{ flashMessage }}
                </div>
            {% endfor %}
            {% for flashMessage in app.session.flashbag.get('danger') %}
                <div  class="alert alert-danger" role="alert">
                    {{ flashMessage }}
                </div>
            {% endfor %}
        </div>

        {% block content %} {% endblock %}

        {% block footer %}
            <footer>
                <p>&copy; 2017 Club Tijuana Xoloitzcuintles de Caliente</p>
            </footer>
        {% endblock %}

    </div>

    {% block javascripts %}

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <script src="/bundles/icfrontend/js/popper.min.js" type="text/javascript"></script>
        <script src="/bundles/icfrontend/js/bootstrap.min.js" type="text/javascript"></script>

        <script src='https://unpkg.com/vue'></script>
        <script src='https://unpkg.com/axios/dist/axios.min.js'></script>
        <script src="/bundles/icfrontend/js/app.js" type="text/javascript"></script>

    {% endblock %}


    </body>
</html>




